package TM5;
public class Persona {
private int personaid; private String nombre; private String seguro; private double
salario;
public Persona(int empleadoid, String nombre, String seguro, double salario) {
this.personaid = personaid; this.nombre = nombre; this.seguro = seguro;
this.salario = salario;
}
